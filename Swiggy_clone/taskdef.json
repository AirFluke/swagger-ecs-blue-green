{
  "family": "swiggy",
  "executionRoleArn": "arn:aws:iam::751647213649:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::751647213649:role/ecsTaskRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "swiggy",
      "image": "<IMAGE1_NAME>",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}